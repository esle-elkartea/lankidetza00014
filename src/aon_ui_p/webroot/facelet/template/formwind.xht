<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core" 
	xmlns:t="http://myfaces.apache.org/tomahawk"	
	xmlns:h="http://java.sun.com/jsf/html"	
	xmlns:ui="http://java.sun.com/jsf/facelets">
<f:loadBundle basename="#{facesContext.application.messageBundle}" var="bundle" />
<f:view>
	<head>
		<title>
			<ui:insert name="title">Title</ui:insert>					
		</title>
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
		<meta http-equiv="Expires" content="0" />
		<meta http-equiv="Pragma" content="no-cache" />
		<meta http-equiv="Cache-Control" content="no-store" />
		<t:stylesheet path="/css/aon.css" />
		<t:stylesheet path="/css/layout.css" />
		<script type='text/javascript' src="${facesContext.externalContext.requestContextPath}/js/Utils.js">
			// nothing
		</script>
		<script type='text/javascript' src="${facesContext.externalContext.requestContextPath}/js/Form.js">
			// nothing
		</script>
		<script type='text/javascript' src="${facesContext.externalContext.requestContextPath}/js/Accelerators.js">
			// nothing
		</script>
		<script type='text/javascript' src="${facesContext.externalContext.requestContextPath}/js/Window.js">
			// nothing
		</script>
		<ui:insert name="head"/>
	</head>
	<body id="aon-body" scroll="no" onload="windowLoaded();" onresize="aonResizeBody();">
		<!-- *********** header ********  -->
		<t:div styleClass="aon-header-container" id="aon-window-popup-header" forceId="#{true}" >	
			<h:panelGrid columns="2" styleClass="aon-header-top"
				columnClasses="aon-header-top-left,aon-header-top-right" dir="true">
				<h:panelGroup>
					<f:verbatim>&#160;</f:verbatim>
				</h:panelGroup>
				<h:panelGroup>
					<h:graphicImage url="/images/header/aon-header-top-right-int.gif" alt="" />
				</h:panelGroup>
			</h:panelGrid>
		
			<h:panelGrid columns="2" styleClass="aon-header-middle"
				columnClasses="aon-header-middle-left,aon-header-middle-right" dir="true">
				<h:panelGroup>
					<t:div id="aon-application" >
						<t:div id="aon-logo" style="padding:0;">
							<h:graphicImage
								url="#{aonConfiguration.applicationLogoContextRelativePath}"
								alt="#{bundle.aon_application_title}" style="height:52px;"
							/>
						</t:div>
						<t:div id="aon-application-name" style="position: relative; left: -35px; top: 35px;  color: #A7A7A8;font-weight:bold;">
							<h:outputText value="#{bundle.aon_application_title}" />
						</t:div>
					</t:div>
				</h:panelGroup>
				<h:panelGroup>
					<f:verbatim>&#160;</f:verbatim>
				</h:panelGroup>
			</h:panelGrid>		
			
			<t:div styleClass="aon-header-bottom">
				<h:graphicImage url="/images/header/aon-header-bottom-right.gif" alt="" />
			</t:div>		
		</t:div>
		<!-- *********** content ********  -->
		<t:div id="aon-content-container-noframes">
			<t:div styleClass="aon-content-container-noframes-int">
				<t:div  styleClass="aon-content-region-without-sidebar">
					<t:div styleClass="aon-errors">
						<t:messages showSummary="false" showDetail="true"/>
					</t:div>
					<ui:insert name="content"/>		
				</t:div>
			</t:div>
		</t:div>
		<!-- *********** footer ********  -->
		<t:div  styleClass="aon-footer-container" id="aon-window-popup-footer" forceId="#{true}">	
			<f:loadBundle basename="#{facesContext.application.messageBundle}" var="bundle" />
			<t:div styleClass="aon-foot-separation">
				<f:verbatim>&#160;</f:verbatim>
			</t:div>
			<h:panelGrid columns="2" styleClass="aon-foot"
		columnClasses="aon-foot-left,aon-foot-right" dir="true">
			<h:panelGroup>
				<t:div styleClass="aon-foot-group" ><h:outputText value="#{bundle.aon_footer_right}" /></t:div>
			</h:panelGroup>
			<h:panelGroup>
				<t:div styleClass="aon-foot-group" style="border-right:0;"><h:outputText value="" /></t:div>
			</h:panelGroup>
		</h:panelGrid>				
		</t:div>		
	</body>
</f:view>	
</html>
