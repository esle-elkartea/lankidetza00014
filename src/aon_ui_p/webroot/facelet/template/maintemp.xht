<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:t="http://myfaces.apache.org/tomahawk"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets">
<f:loadBundle basename="#{facesContext.application.messageBundle}"
	var="bundle" />
<f:loadBundle basename="#{aonConfiguration.applicationBundleBasename}"
	var="applicationBundle" />
<f:loadBundle basename="com.code.aon.ui.hyperview.i18n.messages"
	var="hyperviewBundle" />
<f:view>
	<head>
	<title>#{applicationBundle.aon_application_title}</title>
	<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
	<meta http-equiv="Expires" content="0" />
	<meta http-equiv="Pragma" content="no-cache" />
	<meta http-equiv="Cache-Control" content="no-store" />
	<t:stylesheet path="/css/aon.css" />
	<t:stylesheet path="/css/layout.css" />
	<t:stylesheet path="/css/hyperview.css" />
	<script type='text/javascript'
		src="${facesContext.externalContext.requestContextPath}/js/Hyperview.js">
			// nothing
		</script>
	<script type='text/javascript'
		src="${facesContext.externalContext.requestContextPath}/js/HyperViewForm.js">
			// nothing
		</script>
	</head>
	<body id="aon-body" onload="aonResizeBody()" onresize="aonResizeBody()">
		<ui:insert name="header" />
		
		<t:div id="aon-content-container-noframes" forceId="#{true}">
			<t:div styleClass="aon-content-container-noframes-int">
				<ui:insert name="content" />
			</t:div>
		</t:div>
		<ui:insert name="footer"/>			
	</body>
</f:view>
</html>
